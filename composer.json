{
    "name": "toniette/toniette",
    "description": "A simple framework!",
    "require": {
        "php": "8.*",
        "ext-pdo": "*",
        "ext-json": "*",
        "ext-readline": "*",
        "ext-mysql_xdevapi": "*",
        "toniette/optimizer": "dev-master",
        "toniette/uploader": "dev-master",
        "toniette/datalayer": "dev-master",
        "toniette/paginator": "dev-master",
        "toniette/cropper": "dev-master",
        "toniette/router": "dev-master",
        "vlucas/phpdotenv": ">=5.2",
        "matthiasmullie/minify": ">=1.3.66",
        "league/plates": "^3.3",
        "monolog/monolog": ">=2.2.0",
        "doctrine/cache": "^1.10",
        "robmorgan/phinx": "^0.12.5"
    },
    "autoload": {
        "psr-4": {
            "Source\\": "source/"
        },
        "files": [
          "source/Boot/Config.php",
          "source/Boot/Helpers.php",
          "source/Boot/Boot.php"
        ]
    },
    "license": "MIT",
    "authors": [
        {
            "name": "luispaulotoniettefranca",
            "email": "luis_ti@outlook.com"
        }
    ]
}
